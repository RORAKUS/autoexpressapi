{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "semicolon": true,
    "quotemark": [true, "double", "avoid-escape"],
    "no-var-keyword": true,
    "no-console": false,
    "max-classes-per-file": false,
    "no-empty": false,
    "no-angle-bracket-type-assertion": false
  }
}